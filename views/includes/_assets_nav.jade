	a(href="/organization/assets/locations")
		ul.locationRollUp.pull-left
			li.sum= Math.floor(Math.random()*100+1)
			li.locationLabel Locations

	a(href="/organization/assets/countries")
		ul.locationRollUp.pull-left
			li.sum= Math.floor(Math.random()*193+1)
