extends ../layouts/default

block content
	div.container

		include ../includes/_department_vanity

		.row
			.span12
				include ../includes/_department_details_nav
		.row
			.span12
				#map.shadow
					img(src="/images/map01.png")

		.row
			.span12
				#organizationUsers
					- x = 93
					- for (i=0;i<=10;i++)
						div.userRow.pull-left.incident
							ul.userRisk.pull-left
								li.riskScore= (x--)
								li.riskLabel 
							ul.userMeta.pull-left
								li.username 
									a(href="/locations/locationusername") Location Name
								li.titleDepartment
									span= Math.floor(Math.random()*30+1)
									span &nbsp;Active Users
								li.location Location, Location
							div.pull-right
								ul.userDevices.pull-left
									li.deviceScore= Math.floor(Math.random()*9+1)
									li.deviceLabel Devices
								ul.userClouds.pull-left
									li.cloudScore= Math.floor(Math.random()*30+1)
									li.cloudLabel Cloud Services
								ul.userIncidents.pull-right
									li.incidentScore= Math.floor(Math.random()*9+1)
									li.incideLabel Incidents
					- for (i=0;i<=42;i++)
						a(href="/locations/locationusernames")
							div.userRow.pull-left
								ul.userRisk.pull-left
									li.riskScore= (x--)
									li.riskLabel 
								ul.userMeta.pull-left
									li.username 
										a(href="/locations/locationusername") Location Name
									li.titleDepartment
										span= Math.floor(Math.random()*30+1)
										span &nbsp;Active Users
									li.location Location, Location
								div.pull-right
									ul.userDevices.pull-left
										li.deviceScore= Math.floor(Math.random()*9+1)
										li.deviceLabel Devices
									ul.userClouds.pull-left
										li.cloudScore= Math.floor(Math.random()*30+1)
										li.cloudLabel Cloud Services
									ul.userIncidents.pull-left
										li.incidentScore 0
										li.incideLabel Incidents


block document

	include ../includes/_department_document


	script
		$(function(){
			$("#locationRollUp a:nth-child(3) ul").addClass('active');
			$("#map").show();
		});